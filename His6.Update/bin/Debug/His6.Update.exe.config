<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- 更新程序是否自动退出(0否/1是），默认为1 -->
    <add key="AutoExit" value ="1"/>
    <!-- 更新节点后缀 -->
    <add key="NodeSuffix" value ="1"/>
    <!-- 分支机构 -->
    <add key="BranchId" value="1" />
    <!-- 文件路径 -->
    <add key="Path" value="D:\app\prod" />
    <!--<add key="Path" value="D:\app\test" />-->
    <!-- 中心服务端 HTTP接口和TCP传输地址和端口信息 -->
    <add key="HttpFileURL" value="http://127.0.0.1:9008/"/>
    <add key="TcpFileAddr" value="127.0.0.1"/>
    <add key="TcpFilePort" value="9009"/>
  </appSettings>
</configuration>